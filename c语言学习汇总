#include<stdio.h>
#include<string.h> 
int main()
{
	int input = 0;
	printf("加入比特\n");
	printf("好好学习吗?(1/0)>:");
	scanf_s("%d", &input);
	if (input == 1)
		printf("好offer\n");
	else printf("卖红薯\n");
		return 0;
}
