#include<stdio.h>
#include<string.h>

int main()
{
	printf("%c\n", '\0');
	printf("%d\n", strlen("c:\test\32\test.c"));
	return 0;

}
#include<stdio.h>
#include<string.h> 
int main()
{
	int input = 0;
	printf("加入比特\n");
	printf("好好学习吗?(1/0)>:");
	scanf_s("%d", &input);
	if (input == 1)
		printf("好offer\n");
	else printf("卖红薯\n");
		return 0;
		}
while语句
#include<stdio.h>
int main()
{
	int line = 0;
	printf("加入比特\n");
	while (line < 2000)
	{
		printf("敲一行代码\n");
		line++;
	}
	printf("给我一个好offer\n");
	return 0;
}
函数初识
#include<stdio.h>
int Add(int x, int y)
{
	int z = x + y;
	return z;
}
int main()
{
	int num1 = 10;
	int num2 = 20;
	int num3 = 0;
	int a = 100;
	int b = 200;
	num3 = Add(num1 ,num2);
	num3 = Add(a, b);
	printf("num3=%d\n", num3);

	return 0;
}
